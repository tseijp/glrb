{
        "name": "@glrb/root",
        "author": "tseijp",
        "version": "1.0.0",
        "license": "MIT",
        "private": true,
        "sideEffects": false,
        "scripts": {
                "raymarch": "cd packages/core && ruby raymarch.rb",
                "rspec": "cd packages/core && bundle exec rspec test",
                "rake": "cd packages/core && git add . && rake build",
                "gem": "cd packages/core && gem install pkg/glrb-0.1.0.gem",
                "irb": "yarn rake && yarn gem && irb"
        },
        "engines": {
                "node": ">=14"
        },
        "workspaces": [
                "examples/*",
                "packages/*"
        ]
}
